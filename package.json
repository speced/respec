    "node": ">=8",
    "npm": ">=5"
  },
  "bin": {
    "respec2html": "./tools/respec2html.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/w3c/respec.git"
  },
  "contributors": [
    "Marcos CÃ¡ceres <marcos@marcosc.com> (https://marcosc.com/)",
