  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-modules-amd": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-preset-stage-3-without-async": "^1.0.0",
    "chai": "^4.1.2",
    "clipboard": "^2.0.1",
    "domReady": "github:requirejs/domReady#d85bdc38a6df868ead52ab3e80715aaf60e68765",
    "eslint": "^5.6.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-prettier": "^2.6.2",
    "glob": "^7.1.3",
    "handlebars": "^4.0.12",
    "http-server": "^0.11.1",
    "idb-keyval": "^3.1.0",
    "jasmine-core": "^3.2.1",
    "jasmine-reporters": "^2.3.2",
    "jquery": "^3.3.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-detect-browsers": "^2.3.3",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-opera-launcher": "^1.0.0",
    "karma-requirejs": "^1.1.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-safaritechpreview-launcher": "0.0.6",
    "karma-verbose-summary-reporter": "0.0.1",
    "leaflet": "1.0.3",
    "leaflet-easybutton": "^2.3.0",
    "mocha": "^5.2.0",
    "moment": "^2.22.2",
    "pluralize": "^7.0.0",
    "prettier": "^1.14.3",
    "requirejs": "^2.3.6",
    "text": "github:requirejs/text#d04de4ffd7bf5ba6cb80cdca2d40d4f6f52a1b1f",
    "uglify-es": "3.3.7",
    "webidl2": "^14.0.1"
  },
  "scripts": {
    "babel:build": "babel src -d js --source-maps -q",
    "babel:watch": "babel src -d js --watch --source-maps",
    "build:geo": "npm run copydeps && npm run babel:build && node ./tools/builder.js --profile=geonovum",
    "build:respec-w3c-common": "node ./tools/builder.js --profile=w3c-common",
    "build:components": "npm run copydeps && node ./tools/build-components.js && npm run babel:build",
    "build": "npm run snyk-protect && npm run build:components",
    "copydeps": "node ./tools/copydeps.js",
    "handlebars": "handlebars",
    "hb:build-geo": "node ./tools/expandpaths.js -a js/geonovum/templates/*.html js/geonovum/templates/*.css | xargs handlebars -f js/templates.js",
    "hb:build": "node ./tools/expandpaths.js -a js/*/templates/*.html js/*/templates/*.css js/core/templates/webidl-contiguous/*.html | xargs handlebars -f js/templates.js",
    "highlight:build": "hljs -n --output js/deps/ xml javascript css http markdown json abnf && mv -f js/deps/highlight.pack.js js/deps/highlight.js",
    "karma": "karma start --single-run",
    "lint": "npx eslint src/**/*.js tests/**/*.js",
    "release": "node ./tools/release.js",
    "server": "npm start",
    "snyk": "snyk",
    "snyk-protect": "snyk protect",
    "start": "npm run build:components && http-server -c-1",
    "test:build": "mocha ./tests/test-build.js",
    "test:headless": "node ./tests/headless.js",
    "test:karma": "npm run karma",
    "prepare": "npm run snyk-protect && npm run build:components",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "colors": "^1.3.2",
    "command-line-args": "^5.0.2",
    "command-line-usage": "^5.0.5",
    "epipebomb": "^1.0.0",
    "fs-extra": "^7.0.0",
    "highlight.js": "github:marcoscaceres/highlight.js#7b144b3b83ce296d5b29c5e4fc2ea2a65e9d3f32",
    "hyperhtml": "^2.14.0",
    "loading-indicator": "^2.0.0",
    "marked": "^0.5.1",
    "prompt": "^1.0.0",
    "puppeteer": "^1.8.0",
    "snyk": "^1.99.0"
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "snyk": true
}
